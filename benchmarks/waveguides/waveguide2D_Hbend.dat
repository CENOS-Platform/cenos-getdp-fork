ExtGnuplot = ".dat";
ExtGmsh = ".pos";
myDir = "output/";
catParam1 = "Input/2Geometry/";
catParam2 = "Input/3Signal/";
catParam3 = "Input/4Discretization/";
catOutput = "Output/";

//---------------------------
// Parameters of the geometry
//---------------------------

DIM = 2;

DefineConstant[
  L = { 0.1, Min 0.01, Max 1, Step 0.01,
    Name StrCat[catParam1,"1Length of Domain [m]"]},
  W = { 0.04, Min 0.01, Max 1, Step 0.01,
    Name StrCat[catParam1,"2Width of Domain [m]"]},
  R = { 0.06, Min 0.01, Max 1, Step 0.01,
    Name StrCat[catParam1,"3Radius at bend [m]"]}
];

DefineConstant[
  res = { 1e-3, Min 1e-3, Max 50e-3, Step 1e-3,
    Name StrCat[catParam3,"1Resolution of grid [m]"]}
];

//--------------
// Physical TAGs
//--------------

DOM = 1000;
BND_PEC = 2000;
BND_PORT1 = 2001;
BND_PORT2 = 2002;
